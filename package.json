{
  "name": "kit.modal",
  "version": "1.1.0",
  "description": "Modal window plugin",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "server": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.dev.js && webpack-dev-server --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js &&  cp dist/kit.modal.css prod/kit.modal.css && cp dist/kit.modal.info.css prod/kit.modal.info.css && cp dist/index.html prod/index.html && webpack-dev-server --config webpack.prod.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Natteke/kit.modal.git"
  },
  "author": "Andrey Ponomarenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Natteke/kit.modal/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "homepage": "https://github.com/Natteke/kit.modal#readme"
}
